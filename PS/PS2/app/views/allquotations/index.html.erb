<p id="notice"><%= notice %></p>

<h1>Allquotations</h1>

<table>
  <thead>
    <tr>
      <th>Quote</th>
      <th>Author</th>
      <th>Categories</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allquotations.each do |allquotation| %>
      <tr>
        <td><%= allquotation.quote %></td>
        <td><%= allquotation.author %></td>
        <td><%= allquotation.categories %></td>
        <td><%= link_to 'Show', allquotation %></td>
        <td><%= link_to 'Edit', edit_allquotation_path(allquotation) %></td>
        <td><%= link_to 'Destroy', allquotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Allquotation', new_allquotation_path %>
